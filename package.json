{
  "name": "freemarker-print",
  "description": "print FreeMarker template tokens",
  "version": "0.0.0",
  "author": "sasa+1 <sasaplus1@gmail.com>",
  "dependencies": {
    "commander": "^6.2.1",
    "debug": "^4.3.1",
    "freemarker-parser": "^1.1.6"
  },
  "devDependencies": {
    "@types/node": "^14.14.14",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-node": "^11.1.0",
    "lint-staged": "^10.5.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": ">=12"
  },
  "license": "MIT",
  "private": true,
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/sasaplus1/freemarker-print.git"
  },
  "scripts": {
    "build": "tsc",
    "fix": "run-s fix:eslint fix:prettier",
    "fix:eslint": "eslint --ext .js,.ts --fix .",
    "fix:prettier": "prettier --write --parser typescript './**/*.{js,ts}'",
    "fixpack": "npx fixpack",
    "lint": "run-s lint:eslint lint:prettier",
    "lint:eslint": "eslint --cache --ext .js,.ts .",
    "lint:prettier": "prettier --check --parser typescript './**/*.{js,ts}'",
    "postinstall": "[ -z \"$CI\" ] && git config core.hooksPath .hooks || :",
    "test": "mocha './**/*.test.ts'",
    "watch": "tsc --watch"
  }
}
